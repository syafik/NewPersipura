<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/findzoes_list_text_cellnum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:layout_marginTop="7dp"
        
        android:gravity="center"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:visibility="gone"
        android:singleLine="true"
        
        android:textSize="10dp" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/findzoes_list_text_name"
        android:layout_marginLeft="15dp"
        android:src="@drawable/staff_placeholder2x" />

    <TextView
        android:id="@+id/findzoes_list_text_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/findzoes_list_text_name"
        android:layout_alignBottom="@+id/findzoes_list_text_name"
        android:layout_centerHorizontal="true"
        android:ellipsize="end"
        android:maxLines="2"
        android:paddingBottom="6dp"
        android:text="15:00 WIT"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/findzoes_list_text_address"
        android:layout_marginLeft="14dp"
        android:layout_toRightOf="@+id/imageView1"
        android:paddingTop="6dp"
        android:text="TEAM1"
        android:textSize="14dp" />

    <ImageView
        android:id="@+id/ImageView01"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignRight="@+id/TextView01"
        android:layout_below="@+id/findzoes_list_text_address"
        android:src="@drawable/staff_placeholder2x" />

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/ImageView01"
        android:layout_marginRight="14dp"
        android:layout_toLeftOf="@+id/ImageView01"
        android:paddingTop="6dp"
        android:text="TEAM2"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/TextView04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/findzoes_list_text_address"
        android:layout_centerHorizontal="true"
        android:paddingTop="6dp"
        android:text="vs"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/TextView05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageView01"
        android:layout_alignLeft="@+id/TextView02"
        android:layout_alignRight="@+id/TextView03"
        android:paddingTop="6dp"
        android:text="Stadiun"
        android:gravity="center"
        android:textSize="14dp" />

    <TextView
        android:id="@+id/findzoes_list_text_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignParentTop="true"
        android:paddingTop="6dp"
        android:text="10/8/2013"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginRight="16dp"
        android:paddingTop="6dp"
        android:text="ipl"
        android:textSize="14dp" />

</RelativeLayout>